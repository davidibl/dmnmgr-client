{
  "name": "dmnmgr-client",
  "version": "1.0.4",
  "license": "MIT",
  "author": "David Ibl@xnoname",
  "description": "dmnmgr - A toolkit to work with dmn",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://xnoname.visualstudio.com/schokospeck-client"
  },
  "scripts": {
    "postinstall": "npm run link",
    "ng": "ng",
    "link": "npm link ../web-components/src --production",
    "start": "ng serve --aot=true",
    "start-prod": "ng serve --prod",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "electron": "ng build --prod && electron .",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build-prod && electron .",
    "electron:linux": "npm run build-prod && electron-builder build --linux",
    "electron:windows": "npm run build-prod && electron-builder build --windows",
    "electron:mac": "npm run build-prod && electron-builder build --mac",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "@angular/service-worker": "^7.1.4",
    "@fortawesome/fontawesome-free-webfonts": "1.0.1",
    "breakpoint-sass": "2.7.0",
    "core-js": "^2.4.1",
    "font-awesome": "4.7.0",
    "getbase": "3.1.5",
    "moment": "^2.18.1",
    "openseadragon": "2.2.1",
    "rxjs": "^6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.26",
    "bpmn-js": "3.0.1",
    "dmn-js": "5.2.0",
    "ngx-electron": "2.0.0",
    "rxjs-compat": "^6.0.0-rc.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "^7.1.4",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/language-service": "^7.1.4",
    "@fortawesome/fontawesome-free-webfonts": "1.0.1",
    "@types/electron": "1.6.10",
    "@types/google.analytics": "0.0.35",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "electron": "4.0.0",
    "electron-builder": "20.36.2",
    "electron-reload": "1.3.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~3.1.6",
    "wait-on": "3.2.0"
  }
}
